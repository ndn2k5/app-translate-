<data-extraction-rules>
    <cloud-backup>
        <!-- Allow backup of all data -->
        <include
            domain="file"
            path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Allow transfer of all data -->
        <include
            domain="file"
            path="." />
    </device-transfer>
</data-extraction-rules>

